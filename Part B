import java.util.Random;
import java.util.Scanner;
class Main {
  public static void main(String[] args) {
        Random random = new Random();
        Scanner input = new Scanner(System.in);
        int MIN = 1;
        int MAX = 10;
        int comp = random.nextInt(MAX - MIN + 1) + MIN;
        status = "equal";
        int guesses = 0;

        System.out.print("Think of a number between 1 and 10");
        comp = random.nextInt(MAX - MIN + 1) + MIN;
        System.out.print("Is the number equal, higher, or lower than " + comp + "?: ");
        status = input.nextInt();
        do {
          if (status = higher);
            MIN = comp;
          else if (status = lower);
            MAX = comp;
          else;
            System.out.print("You guessed it!");
        } while (status != equal);
    }
}
